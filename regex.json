{
  "Component": {
    "search": ".*(1)\\.js$",
    "searchDir": "app",
    "test": ".*/templates/(.*)\\.hbs$"
  },
  "Template": {
    "search": ".*(1)\\.hbs$",
    "searchDir": "app",
    "test": ".*/(.*)\\.js$"
  },
  "Route": {
    "search": ".*(1)\\.js",
    "searchDir": "app/routes",
    "test": ".*/(.*)\\.js$"
  }
}
